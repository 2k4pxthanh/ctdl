#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int t;
    cin >> t;
    while (t--)
    {
        int n, m, u, x, y;
        cin >> n >> m >> u;
        vector<pair<int, int>> v;
        vector<vector<int>> a(n + 1);
        vector<int> check(n + 1, 0);
        for (int i = 1; i <= m; i++)
        {
            cin >> x >> y;
            a[x].push_back(y);
            a[y].push_back(x);
        }
        queue<int> q;
        q.push(u);
        check[u] = 1;
        while (q.size())
        {
            u = q.front();
            q.pop();
            for (auto i : a[u])
            {
                if (!check[i])
                {
                    check[i] = 1;
                    q.push(i);
                    v.push_back({u, i});
                }
            }
        }
        if (v.size() == n - 1)
            for (auto i : v)
                cout << i.first << " " << i.second << endl;
        else
            cout << "-1" << endl;
    }
}
